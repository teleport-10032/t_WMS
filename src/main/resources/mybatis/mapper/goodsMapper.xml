<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.t_wms.mapper.goodsMapper">
    <select id="getGoodsList" resultType="goods">
        select product.id,productName,typeName,unit,info,price
            from product,type where type.id = product.typeId;
    </select>
</mapper>
